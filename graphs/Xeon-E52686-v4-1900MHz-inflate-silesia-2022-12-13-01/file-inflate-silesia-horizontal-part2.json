{
  "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
  "title": {
    "text" : "Inflater throughput for Silesia corpus (part2) at compression level 6 on Xeon E5-2686 v4 / 1900MHz",
    "anchor": "middle",
    "offset": 20
  },
  "data": {
    "values" : [
        {"level": "6", "type": "ristretto19-system", "ratio": "3.96", "throughput": "26.20", "file": "samba"},
{"level": "6", "type": "ristretto19-system", "ratio": "3.96", "throughput": "29.31", "file": "samba"},
{"level": "6", "type": "ristretto19-system", "ratio": "3.96", "throughput": "28.52", "file": "samba"},
{"level": "6", "type": "ristretto19-system", "ratio": "3.96", "throughput": "28.81", "file": "samba"},
{"level": "6", "type": "ristretto19-system", "ratio": "3.96", "throughput": "28.91", "file": "samba"},
{"level": "6", "type": "ristretto19-system", "ratio": "60.26", "throughput": "19.20", "file": "xml"},
{"level": "6", "type": "ristretto19-system", "ratio": "60.26", "throughput": "19.31", "file": "xml"},
{"level": "6", "type": "ristretto19-system", "ratio": "60.26", "throughput": "21.20", "file": "xml"},
{"level": "6", "type": "ristretto19-system", "ratio": "60.26", "throughput": "20.37", "file": "xml"},
{"level": "6", "type": "ristretto19-system", "ratio": "60.26", "throughput": "19.47", "file": "xml"}
    ]
  },
  "transform": [
    {
      "filter": "datum.type == 'isal' ? datum.level == 6 : datum.level == 6"
    },
    {
      "aggregate": [
        {
          "op": "mean",
          "field": "throughput",
          "as": "throughput_mean"
        }
      ],
      "groupby": ["type", "file"]
    },
    {
      "lookup": "type",
      "from": {
        "data": {
          "values": [
            {"type": "ristretto19-system", "new-type": "ristretto19-system"}
          ]
        },
        "key": "type",
        "fields": ["new-type"]
      }
    },
    {
      "lookup": "file",
      "from": {
        "data": {
          "values": [
             
          ]
        },
        "key": "file",
        "fields": ["default_val"]
      }
    },
    {
      "calculate": "datum.throughput_mean/datum.default_val",
      "as" : "default-val"
    }
  ],
  "facet": {
    "column": {
      "field": "file",
      "type": "nominal",
      "title": "",
      "header": {
        "labelAngle": 0,
        "labelAlign": "center",
        "labelAnchor": "middle",
        "labelOrient": "bottom"
      },
      "sort": [
        "samba", "xml"
      ]
    }
  },
  "spec": {
    "encoding": {
      "y": {
        "field": "throughput_mean",
        "type": "quantitative",
        "axis": {
          "title": "Throughput (kb/ms)",
          "grid": true
        }
      },
      "x": {
        "field": "new-type",
        "type": "nominal",
        "axis": {
          "labels": false,
          "ticks":false,
          "domainWidth": 0,
          "title": ""
        },
        "sort" : [
          "zlib", "cloudflare", "jtkukunas", "ng", "chromium", "ipp", "isal"
        ]
      }
    },
    "layer": [{
      "mark": "bar",
      "encoding": {
        "color": {
          "field": "new-type",
          "type": "nominal",
          "legend": {
            "title" : "",
            "offset" : null,
            "zindex": 1
          },
          "sort" : [
            "zlib", "cloudflare", "jtkukunas", "ng", "chromium", "ipp", "isal"
          ]
        }
      }
    }, {
      "mark": {
        "type": "text",
        "dx": 5,
        "angle": 270,
        "baseline": "middle",
        "align": "left"  
      },
      "encoding": {
        "text": {
          "field": "default-val",
          "type": "nominal",
          "format": ".0%"
        }
      }
    }]
  },
  "config": {
    "view": {"stroke": "transparent"},
    "axis": {"domainWidth": 1}
  }
}

{"level": "6", "type": "ristretto8-cloudflare", "ratio": "1.40", "throughput": "91.40", "file": "x.ray"}
    ]
  },
  "transform": [
    {
      "filter": "datum.type == 'isal' ? datum.level == 3 : datum.level == 6"
    },
    {
      "aggregate": [
        {
          "op": "mean",
          "field": "throughput",
          "as": "throughput_mean"
        }
      ],
      "groupby": ["type", "file"]
    },
    {
      "lookup": "type",
      "from": {
        "data": {
          "values": [
            {"type": "ristretto8-bundled", "new-type": "ristretto8-bundled"},
{"type": "ristretto8-chromium", "new-type": "ristretto8-chromium"},
{"type": "ristretto8-cloudflare", "new-type": "ristretto8-cloudflare"}
          ]
        },
        "key": "type",
        "fields": ["new-type"]
      }
    },
    {
      "lookup": "file",
      "from": {
        "data": {
          "values": [
             {"file": "x.ray", "default_val": "77.18199999999999"}
          ]
        },
        "key": "file",
        "fields": ["default_val"]
      }
    },
    {
      "calculate": "datum.throughput_mean/datum.default_val",
      "as" : "default-val"
    }
  ],
  "facet": {
    "column": {
      "field": "file",
      "type": "nominal",
      "title": "",
      "header": {
        "labelAngle": 0,
        "labelAlign": "center",
        "labelAnchor": "middle",
        "labelOrient": "bottom"
      },
      "sort": [
        "x.ray"
      ]
    }
  },
  "spec": {
    "encoding": {
      "y": {
        "field": "throughput_mean",
        "type": "quantitative",
        "axis": {
          "title": "Throughput (kb/ms)",
          "grid": true
        }
      },
      "x": {
        "field": "new-type",
        "type": "nominal",
        "axis": {
          "labels": false,
          "ticks":false,
          "domainWidth": 0,
          "title": ""
        },
        "sort" : [
          "ojdk / zlib-bundled", "ojdk / zlib-system", "ojdk / zlib-cloudflare", "ojdk / zlib-chromium"
        ]
      }
    },
    "layer": [{
      "mark": "bar",
      "encoding": {
        "color": {
          "field": "new-type",
          "type": "nominal",
          "legend": {
            "title" : "",
            "offset" : null,
            "zindex": 1
          },
          "sort" : [
            "ojdk / zlib-bundled", "ojdk / zlib-system", "ojdk / zlib-cloudflare", "ojdk / zlib-chromium"
          ]
        }
      }
    }, {
      "mark": {
        "type": "text",
        "dx": 5,
        "angle": 270,
        "baseline": "middle",
        "align": "left"  
      },
      "encoding": {
        "text": {
          "field": "default-val",
          "type": "nominal",
          "format": ".0%"
        }
      }
    }]
  },
  "config": {
    "view": {"stroke": "transparent"},
    "axis": {"domainWidth": 1}
  }
}
